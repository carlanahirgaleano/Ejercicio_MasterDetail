<mvc:View controllerName="EjMasterDetail.EjMasterDetail.controller.Master" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" 	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<Page title="Master App" backgroundDesign="List">
    <content>
        <List mode="SingleSelectMaster" selectionChange="onListItemPress" items="{modelProductos>/value}">
            <items>
                <StandardListItem title="{modelProductos>desc_producto}" type="Active" press="onListItemPress"/>
            </items>
        </List>    
    </content>
</Page>
</mvc:View>